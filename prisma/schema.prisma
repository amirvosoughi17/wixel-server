generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id                Int        @id @default(autoincrement())
  name              String
  phoneNumber       String
  companyName       String
  description       String
  typeOfWeb         WebType
  monthlyUsersCount UsersCount
  likedWebsiteUrls  String[]
}

enum WebType {
  ecommerce
  learning
  company
  personal
  startup
}

enum UsersCount {
  TEN
  FIFTY
  FIVE_HUNDRED
  ONE_THOUSAND
}
